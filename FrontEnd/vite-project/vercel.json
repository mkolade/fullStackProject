{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-builder"
      }
    ],
    "routes": [
      {
        "src": "/src/(.*)",
        "dest": "/src/$1"
      },
      {
        "src": "/(.*).(js|json|ico|css|png|jpe?g|gif|svg|woff2?|ttf|eot)$",
        "dest": "/$1.$2",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  