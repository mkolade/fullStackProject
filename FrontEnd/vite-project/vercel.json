{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*.(js|ico|css|png|jpe?g|gif|svg|woff2?|ttf|eot))",
      "dest": "/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "https://backend-sooty-ten.vercel.app"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
